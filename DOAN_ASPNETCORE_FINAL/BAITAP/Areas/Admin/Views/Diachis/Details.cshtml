@model BAITAP.Models.Diachi

@{
    ViewData["Title"] = "Xem chi tiết";
    Layout = "~/Areas/Admin/Views/Shared/_Layoutadmin.cshtml";
}

<h3 class="mt-0">Chi tiết địa chỉ khách hàng</h3>
<ol class="breadcrumb mb-4">
    <li class="breadcrumb-item"><a asp-action="index">Địa chỉ khách hàng</a></li>
    <li class="breadcrumb-item active">Xem chi tiết</li>
</ol>
<div class="card">
    <div class="card-body">
        <dl class="row">
            <dt class="col-sm-3">@Html.DisplayNameFor(model => model.Diachi1)</dt>
            <dd class="col-sm-9">@Html.DisplayFor(model => model.Diachi1)</dd>
            <dt class="col-sm-3">@Html.DisplayNameFor(model => model.Tinhthanh)</dt>
            <dd class="col-sm-9">@Html.DisplayFor(model => model.Tinhthanh)</dd>
            <dt class="col-sm-3">@Html.DisplayNameFor(model => model.Quanhuyen)</dt>
            <dd class="col-sm-9">@Html.DisplayFor(model => model.Quanhuyen)</dd>
            <dt class="col-sm-3">@Html.DisplayNameFor(model => model.Phuongxa)</dt>
            <dd class="col-sm-9">@Html.DisplayFor(model => model.Phuongxa)</dd>
            <dt class="col-sm-3">@Html.DisplayNameFor(model => model.Macdinh)</dt>
            <dd class="col-sm-9">@Html.DisplayFor(model => model.Macdinh)</dd>
            <dt class="col-sm-3">@Html.DisplayNameFor(model => model.MakhNavigation) </dt>
            <dd class="col-sm-9">@Html.DisplayFor(model => model.MakhNavigation.Ten) | Điện thoại: @Html.DisplayFor(model => model.MakhNavigation.Dienthoai) | Email: @Html.DisplayFor(model => model.MakhNavigation.Email)</dd>
        </dl>

        <div class="text-center">
            <div class="container">
                <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model?.MaDc">Chỉnh Sửa</a> |
                <a class="btn btn-secondary" asp-action="Index">Quay Lại</a>
            </div>

        </div>
    </div>
</div>
