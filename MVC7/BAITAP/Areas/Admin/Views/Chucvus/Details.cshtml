@model BAITAP.Models.Chucvu

@{
    ViewData["Title"] = "Chi Tiết Chức Vụ";
    Layout = "~/Areas/Admin/Views/Shared/_Layoutadmin.cshtml";
}

<h1>Chi Tiết Chức Vụ</h1>

<div class="container">
    <h4>Chức Vụ</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <strong>@Html.DisplayNameFor(model => model.Ten)</strong>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Ten)
        </dd>
        <dt class="col-sm-2">
            <strong>@Html.DisplayNameFor(model => model.Heso)</strong>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Heso)
        </dd>
    </dl>
</div>

<div class="container mt-3">
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model?.Macv">Chỉnh Sửa</a> |
    <a class="btn btn-secondary" asp-action="Index">Quay Lại</a>
</div>
